NORMAL_PUNTO_2_V1 = function(N,MIU,SIGMA,QQ=T){
A = rnorm(N,MIU,SIGMA)
R = table(A>500)/length(A)*100
hist(A, main = "Normal DIstribution")
if (QQ == T) {
return(A)
}else{
return(R)
}
}
